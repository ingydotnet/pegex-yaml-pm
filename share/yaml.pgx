document:
  | block-sequence
  | block-mapping
  | scalar

block-sequence: block-sequence-entry+

block-sequence-entry:
  / DASH SPACE+ scalar EOL /

block-mapping: key-value-pair+ %% EOL

key-value-pair: key mapping-separator value

key: scalar

value: scalar

scalar: /([^: CR NL]+)/

mapping-separator: / COLON SPACE+ /
